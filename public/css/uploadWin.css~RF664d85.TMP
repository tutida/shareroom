#uploadWin{
  position: fixed;
  top: 50px;
  right: 20px;
  z-index: 1000;
  width: 500px;
  height: 275px;
  border:2px
  groove #696969;
  cursor:move;
}

.uploadWinTab{
  overflow:hidden;
  list-style:none;
  padding: 0;
  margin:  0;
  background-color: white
}

.uploadWinTab li{
  background:#66cdaa;
  padding-top: 5px;
  padding-bottom: 5px;
  width: 471px;
  margin-right: 1px;
  font-size: 18px;
  font-weight: bold;
  float:left;
  list-style:none;
  text-align: center;
  cursor:pointer;cursor:hand;
}


#uploadWin-close{
  width: 28px;
  height: 20px;
  margin-right: 0px;
  background-color: #2e8b57;
  color: #fff;
}

ul.uploadWinContents{
  list-style:none;
  padding: 0px;
  margin: 0px;
}

.uploadWinContents li{
  list-style:none;
  width: 500px;
  height: 245px;
  margin: 0px;
  background:#b2ffd8;
}

#delete{
  width: 500px;
  background-color: #000;
  color: #fff
}

#fileBox{
  font-size: 18px;
  font-weight: bold;
  margin: 0px 0px 0px 20px;
}

.thumbnailList{
  margin: 5px 0px 10px 0px;
  width: 480px;
  height: 100px;
  padding: 0px;
}
.thumbnail{
  width: 160px;
  height: 100px;
  padding: 0px;
  float: left;
}

.click {
  max-width: 150px;
  max-height: 80px;
  display: block;
  margin-left: auto;
  margin-right: auto;
  cursor:pointer;cursor:hand
}
.filename {
  font-size: 15px;
  width: 140px;
  height: 20px;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 0px 10px 0px 10px;
}

#uploadButton{
  margin-top: 4px;
  font-size: 20px;
  font-weight: bold;
}

#UploadedFiles{
  height: 200px;
  width: 500px;
  overflow-y: scroll; 
}

.drop {
  opacity: 0.6;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1040;
  background-color: #000000;
}